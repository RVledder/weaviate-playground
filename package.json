{
  "name": "weaviate-playground",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/graphql": "^14.0.1",
    "@types/lodash": "^4.14.116",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.3",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.2",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.1",
    "react-dom": "^16.5.2",
    "react-scripts-ts": "2.17.0",
    "source-map-explorer": "^1.6.0",
    "standard-version": "^4.4.0"
  },
  "scripts": {
    "precommit": "lint-staged && npm test",
    "prepush": "npm test",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "release": "standard-version && npm run build",
    "prettier:check": "prettier --list-different './src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "npm run test:types && npm run test:lint && npm run test:unit",
    "test:lint": "tslint --project ./tsconfig.json",
    "test:types": "tsc --noEmit",
    "test:unit": "CI=true react-scripts-ts test --env=jsdom",
    "test:unit:watch": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.3",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "husky": "^1.0.0-rc.15",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "typescript": "^3.0.3"
  }
}
