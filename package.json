    "@types/lodash.debounce": "^4.0.6",
    "@types/node-fetch": "^2.1.2",
    "@types/prettier": "^1.16.0",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.3",
    "apollo-link-batch-http": "^1.2.11",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-rest": "^0.7.0",
    "apollo-link-state": "^0.4.2",
    "d3": "^5.7.0",
    "d3-force-reuse": "^1.0.1",
    "get-value": "^3.0.1",
    "graphiql": "^0.12.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "jwt-decode": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "nanoid": "^2.0.3",
    "prismjs": "^1.15.0",
    "rc-slider": "^8.6.3",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-google-charts": "^3.0.10",
    "react-router-dom": "^5.0.1",
    "react-scripts-ts": "2.17.0",
    "source-map-explorer": "^1.6.0",
    "standard-version": "^4.4.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "release": "standard-version && npm run build",
    "prettier:check": "prettier --list-different './src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "npm run test:types && npm run test:lint && npm run test:jest",
    "test:lint": "tslint --project ./tsconfig.json",
    "test:types": "tsc --noEmit",
    "test:jest": "CI=true react-scripts-ts test --env=jsdom",
    "test:jest:watch": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/d3": "^5.0.1",
    "@types/get-value": "^3.0.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.3",
    "@types/prismjs": "^1.9.0",
    "@types/puppeteer": "^1.12.3",
    "@types/rc-slider": "^8.6.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "husky": "^1.0.0-rc.15",
    "lint-staged": "^7.3.0",
    "prettier": "^1.16.4",
    "puppeteer": "^1.13.0",
    "typescript": "^3.0.3"
  }
}
